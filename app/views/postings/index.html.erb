<div id="page-content-wrapper">
<div class="container-fluid">
  <div class="row">
  <div class="col-lg-12">
    <h1>All job postings</h1>
  <% @postings.each do |posting| %>
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <%= image_tag "posting.jpg", class:"avatar-square" %>
      <div class="caption">
        <p><%= link_to posting.title, posting_path(posting) %></p>
        <p><%= posting.description %></p>
      </div>
    </div>
  </div>
  <% end %>
  </div>
</div>
</div>
</div>



