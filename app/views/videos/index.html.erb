<h1>Your videos</h1>
<% @videos.each do |video| %>
  <p><%= current_user.first_name  %> <%= current_user.last_name %></p>
  <p><%= link_to video.title, video_path(video) %></p>
  <p><%= video.description %></p>

<% end %>
<%= link_to "Add new video", new_video_path %>
<%= link_to "home", root_path %>
