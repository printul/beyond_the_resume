<div class="container-fluid">
  <p>Sort by date?</p>
  <div class="row">
    <h1>Your Videos</h1>
  <% @videos.each do |video| %>
  <div class="col-sm-6 col-md-4">
    <div class="thumbnail">
      <img src="..." alt="...">
      <div class="caption">
        <p><%= link_to video.title, video_path(video) %></p>
        <p><%= video.description %></p>
      </div>
    </div>
  </div>
  <% end %>
</div>
</div>


